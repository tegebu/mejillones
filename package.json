  "main": "./dist/index.js",
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.json",
    "clean": "rimraf dist && rimraf coverage",
    "commit": "git cz",
    "dev": "ts-node src/index.ts",
    "format": "eslint --fix 'src/**/*.{ts,tsx}'",
    "start": "node dist/index.js",
    "test": "jest test --detectOpenHandles --coverage",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade",
    "update:upgrade": "ncu --upgrade"
  },
  "dependencies": {
    "config": "3.3.3",
    "notion-api-js": "2.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@jamashita/eslint-config": "1.23.1",
    "@types/config": "0.0.38",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@types/sinon": "9.0.10",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "@typescript-eslint/typescript-estree": "4.14.0",
    "eslint": "7.18.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "11.1.0",
    "git-cz": "4.7.6",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "npm-check-updates": "10.2.5",
    "rimraf": "3.0.2",
    "sinon": "9.2.3",
    "sort-package-json": "1.48.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true
}
